{
    "extends": "airbnb",
    "plugins": [
        "compat"
    ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "compat/compat": 2
    }
}
